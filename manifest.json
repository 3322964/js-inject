{
	"manifest_version": 2,
	"name": "Elong页面脚本拦截注入-1.0.0",
	"version": "1.0.0",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"downloads", 
		"<all_urls>", 
		"*://*.elong.com/*",
		"*://*.51vv.com/*",
		"webRequest", 
		"webRequestBlocking",
		"notifications"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["http://*/*"],
			"js": ["jquery-1.9.1.js"],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
		"require.js",
		"returnjs.js",
		"images/*"
	]
}